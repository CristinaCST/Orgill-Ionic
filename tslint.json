{
  "extends": "tslint-ionic-rules/strict",
  
  "rules": {
    "no-duplicate-variable": true,
    "no-unused-variable": [
      true
    ],
    "semicolon": [true, "always"],
    "no-console": false,
    "triple-equals": [true,"always"]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "baseUrl": "./src",
  "paths": {
    "@app/env": [
      "environments/environment"
    ]
  }
}
